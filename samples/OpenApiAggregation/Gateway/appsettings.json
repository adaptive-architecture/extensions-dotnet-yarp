{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Yarp": "Information",
      "AdaptArch.Extensions.Yarp.OpenApi": "Debug"
    }
  },
  "AllowedHosts": "*",
  "Urls": "http://localhost:5000",
  "ReverseProxy": {
    "Routes": {
      "users-get-all": {
        "ClusterId": "user-service",
        "Match": {
          "Path": "/api/users"
        },
        "Metadata": {
          "Ada.OpenApi": "{\"serviceName\":\"User Management\",\"enabled\":true}"
        }
      },
      "users-with-id": {
        "ClusterId": "user-service",
        "Match": {
          "Path": "/api/users/{id}"
        },
        "Metadata": {
          "Ada.OpenApi": "{\"serviceName\":\"User Management\",\"enabled\":true}"
        }
      },
      "products-get-all": {
        "ClusterId": "product-service",
        "Match": {
          "Path": "/api/products"
        },
        "Metadata": {
          "Ada.OpenApi": "{\"serviceName\":\"Product Catalog\",\"enabled\":true}"
        }
      },
      "products-with-id": {
        "ClusterId": "product-service",
        "Match": {
          "Path": "/api/products/{id}"
        },
        "Metadata": {
          "Ada.OpenApi": "{\"serviceName\":\"Product Catalog\",\"enabled\":true}"
        }
      }
    },
    "Clusters": {
      "user-service": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:5001/"
          }
        },
        "Metadata": {
          "Ada.OpenApi": "{\"openApiPath\":\"/swagger/v1/swagger.json\",\"prefix\":\"UserService\"}"
        }
      },
      "product-service": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:5002/"
          }
        },
        "Metadata": {
          "Ada.OpenApi": "{\"openApiPath\":\"/swagger/v1/swagger.json\",\"prefix\":\"ProductService\"}"
        }
      }
    }
  }
}